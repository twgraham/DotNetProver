<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp1.0</TargetFramework>
    <AssemblyName>dotnet-prover</AssemblyName>
    <PackageId>DotNetProver</PackageId>
    <PackageVersion>0.1.1</PackageVersion>
    <Authors>Taylor Graham</Authors>
    <Description>A simple dotnet cli tool integration for getting project package version</Description>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/twgraham/DotNetProver</PackageProjectUrl>
    <RepositoryUrl>https://github.com/twgraham/DotNetProver</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageType>DotnetCliTool</PackageType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.CommandLineUtils" Version="1.1.1" />
  </ItemGroup>
</Project>